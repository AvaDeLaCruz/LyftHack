{
    "manifest_version": 2,
    "name": "LyftCaller",
    "description": "call lyfts with a click of a finger",
    "version": "1.0",

    "browser_action": {
     "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}